{
  "name": "ascentcreative/cms",
  "description": "Core CMS Elements",
  "type": "library",
  "authors": [
    {
      "name": "Ascent Creative",
      "email": "info@ascent-creative.co.uk"
    }
  ],
  "require": {
    "laravel/framework": "^11.0",
    "laravel/fortify": "^1.7",
    "matthiasmullie/minify": "^1.3",
    "spatie/laravel-permission": "^5.0",
    "doctrine/dbal": "^3.0",
    "spatie/laravel-activitylog": "^4.0",
    "spatie/laravel-settings": "^2.1",
    "kalnoy/nestedset": "^6.0",
    "jenssegers/agent": "^2.6",
    "laravel/scout": "^9.3",
    "guizoxxv/laravel-multi-size-image": "^0.2.0" ,
    "ascentcreative/help": "dev-master",
    "ascentcreative/forms": "dev-dev",
    "ascentcreative/sitesearch": "dev-dev"
  },
  "autoload": {
    "psr-4": {
      "AscentCreative\\CMS\\": "src"
    }
  },
  "extra": {
    "laravel": {
        "providers": [
            "AscentCreative\\CMS\\CMSServiceProvider"
        ]
    }
  },
  "repositories": [
    {
        "type": "vcs",
        "url": "git@github.com:ascentcreative/laravel-instagram-feed.git"
      },{
        "type": "vcs",
        "url": "git@github.com:ascentcreative/help.git"
      },{
        "type": "vcs",
        "url": "git@github.com:ascentcreative/pagebuilder.git"
      },{
        "type": "vcs",
        "url": "git@github.com:ascentcreative/sitesearch.git"
      }
    ]
}
